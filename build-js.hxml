-dce full
#Build view js
build-view-js.hxml

-D js-es=6

-lib hxnodejs

-js bin/js/index.js

-main Index

-cmd if [ ! -d bin/js/public ]; then mkdir -p bin/js/public/views ; mkdir bin/js/public/jsha fi
-cmd cp -r src/views/* bin/js/public/views/
-cmd cp -r src/view-js/* bin/js/public/js/

-cmd node bin/js/index.js